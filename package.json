{
  "name": "@crhio/draft3d",
  "version": "0.0.1",
  "description": "A functional toolkit for drawing and flexing 3D parametric drawings",
  "bin": "src/cli.js",
  "scripts": {
    "render": "./src/cli.js",
    "build": "rollup --config",
    "build-app": "rollup --config && npm install --prefix ./app && npm run build --prefix ./app",
    "build-electron-app": "rollup --config && npm run electron-build --prefix ./app",
    "build:watch": "chokidar \"src/**/*.js\" --initial -c \"rollup --config\"",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "author": "CRH Technology Studio",
  "main": "src/index.js",
  "dependencies": {
    "lodash": "^4.17.21",
    "three": "^0.139.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "babel-jest": "^28.1.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.6.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-extended": "^2.0.0",
    "svg-parser": "^2.0.4",
    "vue-jest": "^3.0.7"
  },
  "files": [
    "src"
  ],
  "homepage": "https://github.com/leviat-tech/draft3d#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leviat-tech/draft3d.git"
  }
}
